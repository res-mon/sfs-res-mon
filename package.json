{
  "name": "simple-frontend-stack",
  "version": "0.0.0",
  "description": "",
  "type": "module",
  "scripts": {
    "start": "bun x vite build & bun x vite preview --port 8161",
    "dev": "bun x vite build --watch --minify=false --emptyOutDir false & bun x vite preview --port 8161",
    "build": "bun x vite build",
    "serve": "bun x vite preview --port 8161",
    "update": "bun x npm-check-updates",
    "upgrade": "bun x npm-check-updates --upgrade && bun install",
    "type-check": "bun x tsc --pretty --noEmit",
    "lint": "bun x eslint --fix",
    "lint-check": "bun x eslint",
    "format": "bun x prettier --write \"src/**/*.{ts,tsx,html,css,json}\"",
    "format-check": "bun x prettier --check \"src/**/*.{ts,tsx,html,css,json}\"",
    "prepare": "bun x husky"
  },
  "license": "MIT",
  "devDependencies": {
    "@eslint/compat": "^1.2.7",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.23.0",
    "@felte/solid": "^1.2.14",
    "@formkit/auto-animate": "^0.8.2",
    "@iconify-icon/solid": "^2.3.0",
    "@iconify/json": "^2.2.320",
    "@iconify/tailwind4": "^1.0.6",
    "@kobalte/core": "^0.13.9",
    "@kobalte/tailwindcss": "^0.9.0",
    "@solidjs/router": "^0.15.3",
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.0.15",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/node": "^22.13.13",
    "@types/prismjs": "^1.26.5",
    "@typescript-eslint/eslint-plugin": "^8.27.0",
    "@typescript-eslint/parser": "^8.27.0",
    "@xstate/store": "^3.4.1",
    "ag-grid-community": "^33.1.1",
    "daisyui": "^5.0.9",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.4",
    "eslint-plugin-solid": "^0.14.5",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "import-sort-style-module": "^6.0.0",
    "lucide-solid": "^0.483.0",
    "nanostores": "^0.11.4",
    "npm-check-updates": "^17.1.16",
    "pocketbase": "^0.25.2",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "solid-ag-grid": "^0.0.230",
    "solid-highlight": "^0.1.26",
    "solid-markdown": "^2.0.14",
    "swapy": "^1.0.5",
    "tailwindcss": "^4.0.15",
    "tailwindcss-intersect": "^2.2.0",
    "tailwindcss-motion": "^1.1.0",
    "terser": "^5.39.0",
    "typescript": "^5.8.2",
    "unplugin-icons": "^22.1.0",
    "unplugin-lightningcss": "^0.3.2",
    "vite": "^6.2.3",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-lqip": "^0.0.5",
    "vite-plugin-solid": "^2.11.6",
    "vite-plugin-webfont-dl": "^3.10.4",
    "yup": "^1.6.1"
  },
  "dependencies": {
    "solid-js": "^1.9.5"
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "module",
      "parser": "typescript"
    }
  },
  "sideEffects": [
    "**/@prism*"
  ]
}
