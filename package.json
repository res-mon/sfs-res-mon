{
  "name": "simple-frontend-stack",
  "version": "0.0.0",
  "description": "",
  "type": "module",
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "update": "bun x npm-check-updates",
    "upgrade": "bun x npm-check-updates --upgrade && bun install",
    "type-check": "bun x tsc --pretty --noEmit",
    "lint": "bun x eslint --fix",
    "lint-check": "bun x eslint",
    "format": "prettier --write \"src/**/*.{ts,tsx,html,css,json}\"",
    "format-check": "prettier --check \"src/**/*.{ts,tsx,html,css,json}\"",
    "prepare": "husky"
  },
  "license": "MIT",
  "devDependencies": {
    "@eslint/compat": "^1.2.7",
    "@eslint/eslintrc": "^3.3.0",
    "@eslint/js": "^9.22.0",
    "@felte/solid": "^1.2.14",
    "@formkit/auto-animate": "^0.8.2",
    "@kobalte/core": "^0.13.9",
    "@kobalte/tailwindcss": "^0.9.0",
    "@solidjs/router": "^0.15.3",
    "@tailwindcss/vite": "^4.0.12",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/node": "^22.13.10",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "ag-grid-community": "^33.1.1",
    "daisyui": "^5.0.0",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-solid": "^0.14.5",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "import-sort-style-module": "^6.0.0",
    "lucide-solid": "^0.479.0",
    "nanostores": "^0.11.4",
    "npm-check-updates": "^17.1.15",
    "pocketbase": "^0.25.2",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "solid-ag-grid": "^0.0.230",
    "swapy": "^1.0.5",
    "tailwindcss": "^4.0.12",
    "tailwindcss-intersect": "^2.1.0",
    "tailwindcss-motion": "^1.1.0",
    "typescript": "^5.8.2",
    "vite": "^6.2.1",
    "vite-plugin-lqip": "^0.0.5",
    "vite-plugin-solid": "^2.11.6",
    "vite-plugin-webfont-dl": "^3.10.4",
    "yup": "^1.6.1"
  },
  "dependencies": {
    "solid-js": "^1.9.5"
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "module",
      "parser": "typescript"
    }
  }
}
